{
  "name": "Me Don't Like Facebook",
  "short_name": "MeDontLikeFB",
  "version": "0.0.3",
  "description": "Make your Facebook better, stop liking things.",
  "content_scripts": [{
      "matches": ["*://*.facebook.com/*"],
      "js": ["jquery-1.11.1.min.js", "init.min.js"],
      "run_at": "document_start"
  }],
  "icons": {
    "16": "imgs/icon-16.png",
    "48": "imgs/icon-48.png",
    "128": "imgs/icon-128.png"
  },
  "manifest_version": 2 
}
